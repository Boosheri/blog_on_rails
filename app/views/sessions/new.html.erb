<h1>Sign in</h1>

<%= form_with url: new_session_url, local: true do |f| %>
   <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
   <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
   <%= f.submit "Sign In", class: "btn btn-outline-primary" %>
<% end %>
